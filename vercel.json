{
    "version": 2,
    "builds": [
        {
            "src": "src/**/*.ts",
            "use": "@vercel/node",
            "config": {
                "command": "npm run build",
                "output": "dist"
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "dist/main.js",
            "methods": ["GET", "POST", "PUT", "PATCH", "DELETE"]
        }
    ]
}